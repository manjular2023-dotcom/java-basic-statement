import java.io.*;
import java.util.*;
public class Solution {
public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
        int X1=sc.nextInt();
        int Y1=sc.nextInt();
        int X2=sc.nextInt();
        int Y2=sc.nextInt();
        double midX = (X1 + X2) / 2.0;
        double midY = (Y1 + Y2) / 2.0;
        System.out.println("Binoy's house is located at (" + midX + "," + midY + ")");
         }
}
